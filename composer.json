  "license": "MIT",
  "authors": [
    {
      "name": "Frederick Lawler",
      "homepage": "https://github.com/fredlawl",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/fredlawl/sinsql/issues"
  },
  "require" : {
